-- This file contains several convient functions for working with
-- skeletons.

squashar : (? -> ?) -> ?
squashar = \(x : ? -> ?) -> squash<? -> ?> x;

splitar : ? -> (? -> ?)
splitar = \(x : ?) -> split <? -> ?> x;

squashpd : (?, ?) -> ?
squashpd = \(x : (?, ?)) -> squash<(?, ?)> x;

splitpd : ? -> (?, ?)
splitpd = \(x : ?) -> split <(?, ?)> x;

